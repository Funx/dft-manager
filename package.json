{
  "name": "dft-manager",
  "version": "0.1.0",
  "description": "Simple setup for cycle.js",
  "main": "",
  "scripts": {
    "build-jade": "jade ./src/index.jade -o ./dist/dev/",
    "build-js": "webpack --progress --colors",
    "build": "npm run build-jade && npm run build-js",
    "watch-jade": "nodemon --watch ./src/index.jade --exec 'npm run build-jade'",
    "watch-js": "webpack --progress --watch",
    "watch": "npm run watch-jade & npm run watch-js",
    "dev-server": "webpack-dev-server --content-base dist/dev/ --host 0.0.0.0 --inline --hot --progress --watch",
    "test-server": "webpack-dev-server --hot 'mocha!./test/' --output-filename test.js",
    "build-prod": "webpack --config webpack.production.js -p --progress",
    "build-prod-server": "webpack --config webpack.server.js --progress",
    "preprod-server": "npm run build-prod && npm run build-prod-server",
    "prod-server": "node ./dist/server.js",
    "server": "npm run prod-server",
    "start": "npm run watch-jade & npm run dev-server",
    "test": "NODE_PATH=./src && babel-node ./node_modules/.bin/painless src/{**,**/**}/*.test.js",
    "autotest": "supervisor -q -n exit -x npm run test"
  },
  "author": "GÃ©raud Henrion",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.8",
    "babel-loader": "^5.3.2",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-2": "^6.5.0",
    "css-loader": "^0.19.0",
    "cssnext-loader": "^1.0.1",
    "cycle-restart": "0.0.14",
    "eslint": "^1.0.0",
    "eslint-config-cycle": "^3.0.0",
    "eslint-plugin-cycle": "^1.0.1",
    "eslint-plugin-immutable": "^1.0.0",
    "eslint-plugin-no-class": "^0.1.0",
    "extract-text-webpack-plugin": "^0.8.2",
    "image-webpack-loader": "^1.8.0",
    "jade": "^1.11.0",
    "mocha": "^2.4.5",
    "nodemon": "^1.7.1",
    "painless": "^0.9.4",
    "postcss-loader": "^0.8.2",
    "style-loader": "^0.12.4",
    "supervisor": "^0.9.1",
    "svg-inline-loader": "^0.4.1",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "@cycle/core": "^6.0.3",
    "@cycle/dom": "^9.4.0",
    "@cycle/history": "^2.0.2",
    "@cycle/http": "^8.2.2",
    "@cycle/isolate": "^1.2.0",
    "@cycle/storage": "^2.0.3",
    "diacritics": "^1.2.3",
    "express": "latest",
    "history": "^2.1.0",
    "lost": "latest",
    "next-tick": "^0.2.2",
    "ramda": "latest",
    "rx-combine-latest-obj": "latest",
    "rxjs": "^5.0.0-beta.6",
    "stage": "^0.9.0",
    "switch-path": "latest"
  }
}
